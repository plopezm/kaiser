{
    "version": "1",
    "name": "stringifyJson",
    "args": [
        {
            "name": "me",
            "value": {
                "name": "Pablo Lopez"
            }
        }
    ],
    "start": "printHello",
    "tasks": {
        "printHello": {
            "script": "console.log('Hello me '+ me);console.log('Hello stringify ' + JSON.stringify(me));",
            "onSuccess": "sayGoodbye",
            "onFailure": "sayError"
        },
        "sayGoodbye": {
            "script": "console.log('bye bye')"
        },
        "sayError": {
            "script": "console.log('bye bye with error')"
        }
    }
}